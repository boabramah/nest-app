    "@nestjs/swagger": "^4.7.13",
    "@nestjs/typeorm": "^7.1.5",
    "@types/nunjucks": "^3.1.4",
    "@vue/server-renderer": "^3.0.5",
    "bcrypt": "^5.0.0",
