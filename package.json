    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sass-loader": "^11.0.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31",
    "vue": "^3.0.5",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.4"
